   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="org.rssowl.ui.fileSet"
            label="File"
            visible="true">
         <action
               class="org.rssowl.ui.internal.actions.NewTypeDropdownAction"
               icon="icons/etool16/add.gif"
               id="org.rssowl.ui.NewType"
               label="New"
               style="pulldown"
               toolbarPath="file"
               tooltip="New">
         </action>
         <action
               class="org.rssowl.ui.internal.actions.NewBookMarkAction"
               definitionId="org.rssowl.ui.actions.NewBookMark"
               icon="icons/obj16/bookmark.gif"
               id="org.rssowl.ui.actions.NewBookMark"
               label="Bookmark"
               menubarPath="file/new_sub/bookmark"
               style="push">
         </action>
         <action
               class="org.rssowl.ui.internal.actions.NewSearchMarkAction"
               definitionId="org.rssowl.ui.actions.NewSearchMark"
               icon="icons/obj16/searchmark.gif"
               id="org.rssowl.ui.actions.NewSearchMark"
               label="Saved Search"
               menubarPath="file/new_sub/searchmark"
               style="push">
         </action>
         <action
               class="org.rssowl.ui.internal.actions.NewFolderAction"
               definitionId="org.rssowl.ui.actions.NewFolder"
               icon="icons/obj16/folder.gif"
               id="org.rssowl.ui.actions.NewFolder"
               label="Folder"
               menubarPath="file/new_sub/folder"
               style="push">
         </action>
         <action
               class="org.rssowl.ui.internal.actions.ExportFeedsAction"
               definitionId="org.rssowl.ui.actions.ExportFeeds"
               icon="icons/etool16/export.gif"
               id="org.rssowl.ui.actions.ExportFeeds"
               label="&amp;Export Feeds..."
               menubarPath="file/additions"
               style="push"
               tooltip="Export Feeds to an OPML source"/>
         <action
               class="org.rssowl.ui.internal.actions.ImportFeedsAction"
               definitionId="org.rssowl.ui.actions.ImportFeeds"
               icon="icons/etool16/import.gif"
               id="org.rssowl.ui.actions.ImportFeeds"
               label="&amp;Import Feeds..."
               menubarPath="file/additions"
               style="push"
               tooltip="Import Feeds from an OPML source"/>
         <menu
               id="new_sub"
               label="&amp;New"
               path="file/new.ext">
            <groupMarker
                  name="bookmark" />
            <groupMarker
                  name="searchmark">
            </groupMarker>
            <separator
                  name="folder">
            </separator>
         </menu>
      </actionSet>
      <actionSet
            id="org.rssowl.ui.editSet"
            label="Edit"
            visible="true">
         <action
               class="org.rssowl.ui.internal.actions.SearchNewsAction"
               definitionId="org.rssowl.ui.SearchNewsAction"
               icon="icons/obj16/searchmark.gif"
               id="org.rssowl.ui.SearchNewsAction"
               label="Search News"
               menubarPath="edit/editEnd"
               style="push"
               toolbarPath="search">
         </action>
         <action
               allowLabelUpdate="true"
               definitionId="org.rssowl.ui.FindAction"
               icon="icons/etool16/find.gif"
               id="org.rssowl.ui.FindAction"
               label="&amp;Find"
               menubarPath="edit/editEnd"
               retarget="true"
               style="push"
               >
         </action>
      </actionSet>
      <actionSet
            id="org.rssowl.ui.goSet"
            label="Go"
            visible="true">
