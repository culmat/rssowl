   <extension
         point="org.eclipse.ui.bindings">
      <scheme
            description="This is the RSSOwl Keys Scheme"
            id="org.rssowl.ui.KeyScheme"
            name="RSSOwl Keys"/>
      <key
            commandId="org.eclipse.ui.file.close"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.rssowl.ui.KeyScheme"
            sequence="M1+W"/>
      <key
            commandId="org.eclipse.ui.file.closeAll"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.rssowl.ui.KeyScheme"
            sequence="M1+M2+W"/>
      <key
            commandId="org.rssowl.ui.actions.Reload"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.rssowl.ui.KeyScheme"
            sequence="M1+U"/>
      <key
            commandId="org.rssowl.ui.actions.ReloadAll"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.rssowl.ui.KeyScheme"
            sequence="M1+M2+U"/>
      <key
            commandId="org.eclipse.ui.edit.selectAll"
            contextId="org.eclipse.ui.contexts.dialogAndWindow"
            schemeId="org.rssowl.ui.KeyScheme"
            sequence="M1+A"/>
      <key
            commandId="org.eclipse.ui.edit.delete"
            contextId="org.eclipse.ui.contexts.dialogAndWindow"
            schemeId="org.rssowl.ui.KeyScheme"
            sequence="DEL"/>
      <key
            commandId="org.rssowl.ui.MarkAllRead"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.rssowl.ui.KeyScheme"
            sequence="M1+M2+C"/>
      <key
            commandId="org.eclipse.ui.file.properties"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.rssowl.ui.KeyScheme"
            sequence="M3+Enter">
      </key>
      <key
            commandId="org.rssowl.ui.FindAction"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.rssowl.ui.KeyScheme"
            sequence="M1+F">
      </key>
      <key
            commandId="org.rssowl.ui.NextUnreadNews"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.rssowl.ui.KeyScheme"
            sequence="M1+SPACE">
