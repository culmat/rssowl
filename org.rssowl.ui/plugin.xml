               path="tools/additions">
            <separator name="extensionsBegin"/>
            <groupMarker name="findExtensions"/>
            <separator name="extensionsEnd"/>
            <groupMarker name="manageConfiguration"/>
         </menu>
      </actionSet>
      <actionSet
            id="org.rssowl.ui.NewsSet"
            label="News"
            visible="true">
         <action
               allowLabelUpdate="true"
               definitionId="org.rssowl.ui.MarkAllRead"
               icon="icons/elcl16/mark_all_read.gif"
               id="org.rssowl.ui.MarkAllRead"
               label="Mark All Read"
               menubarPath="news/mark"
               retarget="true"
               style="push"
               toolbarPath="news/mark"/>
         <action
               class="org.rssowl.ui.internal.actions.MarkReadAction"
               definitionId="org.rssowl.ui.actions.MarkRead"
               disabledIcon="icons/dlcl16/mark_read.gif"
               enablesFor="+"
               icon="icons/elcl16/mark_read.gif"
               id="org.rssowl.ui.actions.MarkRead"
               label="&amp;Mark Read"
               menubarPath="news/mark"
               style="push"
               toolbarPath="news/mark"
               tooltip="Mark selected Items as Read"/>
         <action
               class="org.rssowl.ui.internal.actions.ReloadAllAction"
               definitionId="org.rssowl.ui.actions.ReloadAll"
               icon="icons/elcl16/reload_all.gif"
               id="org.rssowl.ui.actions.ReloadAll"
               label="Update All"
               menubarPath="news/reload"
               style="push"
               toolbarPath="news/reload"/>
         <action
               definitionId="org.rssowl.ui.actions.Reload"
               disabledIcon="icons/dlcl16/reload.gif"
               icon="icons/elcl16/reload.gif"
               id="org.rssowl.ui.actions.Reload"
               label="Update"
               menubarPath="news/reload"
               retarget="true"
               style="push"
               toolbarPath="news/reload"/>
         <action
